{
  "name": "tbdemoelectron",
  "version": "1.0.17",
  "description": "tb demo electron",
  "scripts": {
    "postinstall": "install-app-deps",
    "clean": "rimraf dist",
    "start": "electron ./app --enable-logging",
    "dev": "SET NODE_ENV=development && npm run start",
    "postversion": "git push && git push --tags",
    "release-patch": "npm version patch",
    "prerelease-patch": "cd app && npm version patch --no-git-tag-version && git commit -a -m prepatch",
    "dist-osx": "build --osx --x64 --ia32",
    "pack-win64": "electron-packager ./app --asar --platform win32 --arch x64 --out dist --overwrite",
    "prepack-win64": "npm run clean",
    "dist-win64": "electron-installer-windows --src dist/tbdemoelectron-win32-x64 --dest dist/installers/win32-x64 --config config.json",
    "predist-win64": " npm run pack-win64",
    "upload-win64": "node scripts/upload-win64.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tboeker/tbelectrondemo.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "demo",
    "tb"
  ],
  "author": "TB",
  "bugs": {
    "url": "https://github.com/tboeker/tbelectrondemo/issues"
  },
  "homepage": "https://github.com/tboeker/tbelectrondemo",
  "devDependencies": {
    "electron-builder": "^6.3.1",
    "electron-packager": "^7.7.0",
    "electron-prebuilt": "^1.2.0",
    "electron-installer-windows": "^0.2.0",
    "fs-plus": "*",
    "rimraf": "*",
    "yargs": "*",
    "publish-release": "*",
    "multi-glob": "*",
    "gh-release-assets" : "*"
  },
  "build": {
    "appId": "tbdemo.electron.1",
    "app-category-type": "public.app-category.productivity",
    "win": {
      "iconUrl": "https://github.com/favicon.ico",
      "remoteReleases": "true"
    }
  },
  "dependencies": {}
}
