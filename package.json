{
  "name": "tbdemoelectron",
  "version": "1.0.37",
  "description": "tb demo electron",
  "scripts": {
    "postinstall": "install-app-deps",
    "clean": "rimraf dist",
    "compile": "npm run clean",
    "start": "electron ./app --enable-logging",
    "dev": "SET NODE_ENV=development && npm run start",
    "postversion": "git push && git push --tags",
    "prerelease-patch": "cd app && npm version patch --no-git-tag-version && git commit -a -m \"App Release Patch Upgrade %s\"",
    "release-patch": "npm version patch -m \"Release Patch Upgrade %s\"",
    "pack-osx": "npm run compile && build --osx --x64 --ia32 --dir",
    "dist-osx": "npm run compile &&  build --osx --x64 --ia32",
    "pack-win64": "npm run compile && build --win --x64 --dir",
    "dist-win64": "npm run compile && build --win --x64",
    "upload-win64": "node scripts/upload-win64.js",
    "pack-win1": "electron-packager ./app --asar --platform win32 --arch x64 --out dist --overwrite",
    "prepack-win1": "npm run clean",
    "dist-win1": "electron-installer-windows --src dist/tbdemoelectron-win32-x64 --dest dist/installers/win32-x64 --config config.json"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tboeker/tbelectrondemo.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "demo",
    "tb"
  ],
  "author": "TB",
  "bugs": {
    "url": "https://github.com/tboeker/tbelectrondemo/issues"
  },
  "homepage": "https://github.com/tboeker/tbelectrondemo",
  "devDependencies": {
    "electron": "*",
    "electron-builder": "*",
    "electron-installer-squirrel-windows": "*",
    "electron-installer-windows": "*",
    "electron-packager": "*",
    "electron-prebuilt": "*",
    "electron-winstaller": "*",
    "multi-glob": "*",
    "pkginfo": "*",
    "publish-release": "*",
    "rimraf": "*"
  },
  "build": {
    "appId": "tb.demo.electron",
    "category": "public.app-category.productivity",
    "win": {
      "certificateFile": "tbdemoelectron.pfx",
      "certificatePassword": "Password"
    }
  },
  "dependencies": {
    "winston": "*",
    "electron-squirrel-startup": "*",
    "electron-gh-releases": "*"
  }
}
